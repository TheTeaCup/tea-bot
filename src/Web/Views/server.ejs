<!-- Content -->
	<div class="index-container" style="background-color: none;">
		<div class="text-center">
		
			<img class="server-avatar" src="<%= Guild.iconURL; %>" style="outline-style:red;" onerror="this.onerror=null;this.src='https://i.imgur.com/2otMem9.png';">
			<p class="server-name" style="outline-style:black;" title="<%= Guild.name %>"><%= Guild.name %></p>
<% if(Tags) { %>
			 <ul class="tags">
<% for(var i=0; i < Tags.length; i++) { %> 
        <a class="btn btn-success" href="/tags?q=<%- Tags[i] %>"><%-	Tags[i] %></a>	
<% } %>
			 </ul>
<% } %>
		</div>
	</div>

	<div class="container" style="background-color: none;">
		<div class="row server-container">
		  <div class="col-lg-3">
		  
			<div class="card server-info">
			  <div class="card-body">
				<h5 class="le card-title" styele="font-size:25px;">Server Info</h5>
				<ul class="list-group list-group-flush">
          <li class="le list-group-item">Owner: <a href="/user/<%= Guild.owner.id %>"><%= Mythical.users.get(Guild.owner.id).tag%></a></li>
				  <li class="le list-group-item">Status: <%= Status %></li>
				  <li class="le list-group-item">Members: <%= Guild.memberCount %></li>
       <li class="le list-group-item">Online: <%= Guild.members.filter(m => m.presence.status === 'online').size %></li>
          <% if(timeObj === "n/a") { %>  <li class="le list-group-item">Last Bump: Never</li> <% } else { %>
          <% if(timeObj.hours === 0) { %> <li class="le list-group-item">Last Bumped: <%= timeObj.minutes %> mins ago</li> <% } else { %> <li class="le list-group-item">Last Bumped: <%= timeObj.hours %> hours ago</li> <% } %>
          <% } %>
				</ul>
			  </div>
			</div>
      <% if(user){ %>
      <a class="btn btn-bump btn-block" onclick=goto("/server/<%= Guild.id %>/bump") style="margin-bottom: 15px; background:#b2c8d2" rel="nofollow" >Bump the server</a>
      <% } %>
			<a class="btn btn-success btn-block" onclick=goto("/server/<%= Guild.id %>/invite") style="margin-bottom: 15px;" rel="nofollow" target="_blank">Join the server</a> 
        <% if(website) { %>
          <a class="btn btn-success btn-block" href="<%- website %>" style="margin-bottom: 15px;" rel="nofollow" target="_blank">Website</a> 
        <% } %>
      <% if(user){ %>
        <% if(Allowed.includes(user.id)) { %>
        <br><hr>  <p class="mb-0" style="color:yellow">User Controls</p>
        <a class="btn btn-warning btn-block" onclick=goto("/server/<%= Guild.id %>/edit") style="margin-bottom: 15px;" rel="nofollow" target="_blank">Edit Server</a>
        <% } %>
          <% if(Mythical.Staff.includes(user.id)) { %>
         <br><hr>  <p class="mb-0" style="color:yellow">Staff Controls</p>
          <a class="btn btn-danger btn-block" onclick=goto("/server/<%= Guild.id %>/removedescs") style="margin-bottom: 15px;" rel="nofollow" target="_blank">Moderate All Descriptions</a>
          <a class="btn btn-success btn-block" onclick=goto("/server/<%= Guild.id %>/approve") style="margin-bottom: 15px;" rel="nofollow" target="_blank">Approve Server</a>
       <% if(Mythical.Developers.includes(user.id)) { %>
        <br><hr><p class="mb-0" style="color:yellow">Admin Controls</p>
          <a class="btn btn-blue btn-block" onclick=goto("/server/<%= Guild.id %>/feature") style="margin-bottom: 15px;" rel="nofollow" target="_blank">Feature Server</a>
          <a class="btn btn-orange btn-block" onclick=goto("/server/<%= Guild.id %>/official") style="margin-bottom: 15px;" rel="nofollow" target="_blank">Verify Server</a>
       
           
          <% } %>
       <% } %>
      <% } %>
		  </div>
		  <div class="col-lg-9">
                  
<% if(UpV === "123"){ %>
       <br>
<div class="container">
 <center>
   <div class="row" role="alert">
     <div class="col-md-12" role="alert">
       <div class="alert alert-primary alert-success" role="alert">
         <button type="button" class="close" data-dismiss="alert">×</button>
         <h4 class="alert-heading">Done!</h4>
         <p class="mb-0">You Upvoted <%= Guild.name %></p>
      </div>
    </div>
  </div>
 </center>
</div>
<% } %>
        
<% if(hr){ %>
       <br>
<div class="container">
 <center>
   <div class="row" role="alert">
     <div class="col-md-12" role="alert">
       <div class="alert alert-primary alert-danger" role="alert">
         <button type="button" class="close" data-dismiss="alert">×</button>
         <h4 class="alert-heading">Oops, there is a problem..</h4>
         <p class="mb-0">You can upvote this server in <bold><%= hr %> Hours(s) & <%= min %> Minute(s)</bold></p>
      </div>
    </div>
  </div>
 </center>
</div>
<% } else if(min){ %>
       <br>
<div class="container">
 <center>
   <div class="row" role="alert">
     <div class="col-md-12" role="alert">
       <div class="alert alert-primary alert-danger" role="alert">
         <button type="button" class="close" data-dismiss="alert">×</button>
         <h4 class="alert-heading">Oops, there is a problem..</h4>
         <p class="mb-0">You can upvote this server in <bold><%= min %> Minute(s)</bold></p>
      </div>
    </div>
  </div>
 </center>
</div>
<% } %>
        
